@model IEnumerable<DataAccessLayer.Data.Models.ApplicationUser>
@{
    Layout = "_AdminLayout";
}
<h2>Admin List</h2>

<table class="table">
    <thead>
        <tr>
            <th>Username</th>
            <th>Email</th>
            <th>First Name</th>
            <th>Last Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var admin in Model)
        {
            <tr>
                <td>@admin.UserName</td>
                <td>@admin.Email</td>
                <td>@admin.FirstName</td>
                <td>@admin.LastName</td>
            </tr>
        }
    </tbody>
</table>

@if (!Model.Any())
{
    <p>No admins found.</p>
}
